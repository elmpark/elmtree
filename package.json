{
  "name": "elmtree",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall.web && ng serve web-elmtree",
    "build": "npm run postinstall.web && ng build web-elmtree",
    "test": "npm run postinstall.web && ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "start.web.elmtree": "npm run postinstall.web && ng serve web-elmtree",
    "postinstall": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true npx electron-builder install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.elmtree": "npm run prepare.electron.elmtree && ng build electron-elmtree --prod --base-href ./",
    "build.electron.elmtree.local": "npm run build.electron.elmtree && electron dist/apps/electron-elmtree",
    "build.electron.elmtree.linux": "npm run build.electron.elmtree && cd dist/apps/electron-elmtree && npx electron-builder build --linux",
    "build.electron.elmtree.windows": "npm run build.electron.elmtree && cd dist/apps/electron-elmtree && npx electron-builder build --windows",
    "build.electron.elmtree.mac": "npm run build.electron.elmtree && cd dist/apps/electron-elmtree && npx electron-builder build --mac",
    "prepare.electron.elmtree": "npm run postinstall.electron && tsc -p apps/electron-elmtree/tsconfig.json",
    "serve.electron.elmtree.target": "ng serve electron-elmtree",
    "serve.electron.elmtree": "wait-on http-get://localhost:4200/ && electron apps/electron-elmtree/src --serve",
    "start.electron.elmtree": "npm run prepare.electron.elmtree && npm-run-all -p serve.electron.elmtree.target serve.electron.elmtree"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@ngrx/effects": "6.1.2",
    "@ngrx/router-store": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@nrwl/nx": "7.3.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26",
    "@ngx-translate/core": "~11.0.0",
    "@ngx-translate/http-loader": "~4.0.0",
    "@elmtree/scss": "file:libs/scss",
    "reflect-metadata": "^0.1.12",
    "@elmtree/web": "file:xplat/web"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@ngrx/store-devtools": "6.1.2",
    "@nrwl/schematics": "7.3.0",
    "@nstudio/schematics": "^7.2.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "electron": "2.0.8",
    "electron-builder": "20.28.4",
    "electron-installer-dmg": "1.0.0",
    "electron-packager": "12.1.0",
    "electron-reload": "1.2.5",
    "electron-store": "2.0.0",
    "electron-updater": "3.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "^4.1.5",
    "npx": "10.2.0",
    "prettier": "1.15.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "2.1.0"
  },
  "xplat": {
    "prefix": "et"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
